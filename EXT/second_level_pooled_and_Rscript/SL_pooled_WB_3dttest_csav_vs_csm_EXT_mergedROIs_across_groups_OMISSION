#!/bin/tcsh -xef

# created by uber_ttest.py: version 2.1 (May 11, 2020)
# creation date: Fri Feb 11 10:02:02 2022

# ---------------------- set process variables ----------------------

set dirA = derivatives/afni
set dirP = derivatives/fmriprep

# specify and possibly create results directory
set results_dir = $dirA/SL/EXT
if ( ! -d $results_dir ) mkdir $results_dir

set mask_dset = $dirA/mergedROIs_resam.nii

if ( -e $results_dir/SL_pooled_WB_3dttest_csav_vs_csm_EXT_mergedROIs_across_groups_OMISSION+tlrc.BRIK ) rm $results_dir/SL_pooled_WB_3dttest_csav_vs_csm_EXT_mergedROIs_across_groups_OMISSION+tlrc.*
# ------------------------- process the data -------------------------

3dttest++ -Clustsim -prefix $results_dir/SL_pooled_WB_3dttest_csav_vs_csm_EXT_mergedROIs_across_groups_OMISSION                        \
          -mask $mask_dset\
