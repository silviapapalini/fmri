#!/bin/tcsh -xef

# created by uber_ttest.py: version 2.1 (May 11, 2020)
# creation date: Thu Apr 14 07:10:10 2022

# ---------------------- set process variables ----------------------

set dirA = /home/thaber/FASTING_MRI_BIDS/derivatives/afni
set dirP = /home/thaber/FASTING_MRI_BIDS/derivatives/fmriprep

# specify and possibly create results directory
set results_dir = $dirA/SL/EXT
if ( ! -d $results_dir ) mkdir $results_dir

set mask_dset = $dirA/mergedROIs_resam.nii

if ( -e $results_dir/SL_PM_VW_merged_ROIs_ttest_omissioncsmnonmod_vs_omissioncsavnonmod_diff_groups ) rm $results_dir/SL_PM_VW_merged_ROIs_ttest_omissioncsmnonmod_vs_omissioncsavnonmod_diff_groups

# ------------------------- process the data -------------------------

3dttest++ -prefix $results_dir/SL_PM_VW_merged_ROIs_ttest_omissioncsmnonmod_vs_omissioncsavnonmod_diff_groups -AminusB         \
          -mask $mask_dset \
